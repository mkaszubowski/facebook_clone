<%= link "New message", to: "#", class: "show-message-form" %>
<%= render "_form.html", conn: @conn, changeset: @changeset %>

<h1>Conversations</h1>

<ul>
  <%= for conversation <- @conversations do %>
    <li>
      <%= conversation_link(@conn, conversation, @current_user) %>
    </li>
  <% end %>
</ul>
